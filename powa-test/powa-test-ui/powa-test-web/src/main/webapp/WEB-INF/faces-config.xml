<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<application>
	</application>

	<!-- Frontend team navigation rules -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>REGISTRATION_FIRST_PAGE</from-outcome>
			<to-view-id>/pages/registration/firstPage.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>REGISTRATION_SECOND_PAGE</from-outcome>
			<to-view-id>/pages/registration/secondPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
